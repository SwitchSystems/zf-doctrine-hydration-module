{
  "name": "phpro/zf-doctrine-hydration-module",
  "license": "MIT",
  "description": "Doctrine hydrators for ZF2 and ZF3",
  "keywords": [
    "zf",
    "doctrine",
    "hydrator"
  ],
  "authors": [
    {
      "name": "Toon Verwerft",
      "email": "toon.verwerft@phpro.be"
    }
  ],
  "extra": {
    "zf": {
      "module": "Phpro\\DoctrineHydrationModule"
    }
  },
  "require": {
    "php": "^8.0",
    "doctrine/common": "^3.4",
    "doctrine/doctrine-module": "^5.0",
    "doctrine/instantiator": "^1.5",
    "laminas/laminas-servicemanager": "^3.3",
    "laminas/laminas-hydrator": "^4.0",
    "laminas/laminas-modulemanager": "^2.7"
  },
  "require-dev": {
    "fabpot/php-cs-fixer": "^2.19",
    "phpunit/phpunit": "^8.5",
    "doctrine/doctrine-orm-module": "^5.0",
    "doctrine/doctrine-mongo-odm-module": "^4.0",
    "doctrine/mongodb-odm": "^2.0",
    "phpro/grumphp": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Phpro\\DoctrineHydrationModule\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhproTest\\DoctrineHydrationModule\\": "test/src/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
